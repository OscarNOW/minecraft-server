{
    "name": "@boem312/minecraft-server",
    "version": "1.4.0",
    "description": "A pure JS library to create Minecraft Java 1.16.3 servers",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "node ./scripts/build",
        "minify": "node ./scripts/minify",
        "readme": "node ./scripts/readme",
        "test": "node ./scripts/test",
        "types": "node ./scripts/types",
        "updateVersionFiles": "node ./scripts/updateVersionFiles",
        "docs": "node ./src/docs/generate",
        "eslint": "npx eslint ./src/ ./examples/ ./scripts/ index.js --max-warnings 0"
    },
    "keywords": [
        "minecraft",
        "server",
        "minecraft-server",
        "protocol",
        "wrapper"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OscarNOW/minecraft-server.git"
    },
    "author": "Oscar",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/OscarNOW/minecraft-server/issues/new/choose"
    },
    "homepage": "https://github.com/OscarNOW/minecraft-server#readme",
    "dependencies": {
        "axios": "^1.4.0",
        "endian-toggle": "^0.0.0",
        "image-size": "^1.0.2",
        "minecraft-protocol": "^1.41.2",
        "prismarine-chunk": "^1.33.0",
        "protodef": "^1.15.0"
    },
    "devDependencies": {
        "eslint": "^8.40.0",
        "mineflayer": "^4.8.1",
        "typedoc": "^0.24.7",
        "typedoc-theme-hierarchy": "^3.2.0"
    },
    "overrides": {
        "jsonwebtoken": "9.0.0"
    }
}
