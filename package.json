{
    "name": "@boem312/minecraft-server",
    "version": "1.4.6",
    "description": "A pure JS library to create Minecraft Java 1.16.3 servers",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "node ./scripts/build",
        "minify": "node ./scripts/minify",
        "readme": "node ./scripts/readme",
        "test": "node ./scripts/test",
        "types": "node ./scripts/types",
        "updateVersionFiles": "node ./scripts/updateVersionFiles",
        "docs": "node ./src/docs/generate",
        "eslint": "npx eslint ./src/ ./examples/ ./scripts/ index.js --max-warnings 0"
    },
    "keywords": [
        "minecraft",
        "server",
        "minecraft-server",
        "protocol",
        "wrapper"
    ],
    "repository": "github:OscarNOW/minecraft-server",
    "engines": {
        "node": "^18 || ^20"
    },
    "author": {
        "name": "Oscar Knap",
        "url": "https://github.com/OscarNOW"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/OscarNOW/minecraft-server/issues/new/choose"
    },
    "homepage": "https://oscarnow.github.io/minecraft-server/",
    "dependencies": {
        "axios": "^1.7.2",
        "endian-toggle": "^0.0.0",
        "image-size": "^1.1.1",
        "minecraft-protocol": "^1.46.0",
        "prismarine-chunk": "^1.35.0",
        "protodef": "^1.15.0"
    },
    "devDependencies": {
        "mineflayer": "^4.20.1",
        "eslint": "^9.6.0",
        "typedoc": "^0.26.3",
        "typedoc-theme-hierarchy": "^5.0.3"
    },
    "overrides": {
        "jsonwebtoken": "9.0.0"
    }
}