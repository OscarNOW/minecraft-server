{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-undef": "error",
        "no-undefined": "error",
        "no-unused-vars": "warn",
        "no-const-assign": "error"
    }
}